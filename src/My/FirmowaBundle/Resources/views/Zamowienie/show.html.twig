{% extends '::base.html.twig' %} 
{% block body %}
<h1>Zamowienie</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Data_przyjecia</th>
            <td>{{ entity.dataprzyjecia|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Data_odbioru</th>
            <td>{{ entity.dataodbioru|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Usterka</th>
            <td>{{ entity.Usterka }}</td>
        </tr>
        <tr>
            <th>Numer_zamowienia</th>
            <td>{{ entity.numerzamowienia }}</td>
        </tr>
        <tr>
            <th>Klient</th>
            <td>{{ entity.klienci }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('zamowienie') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('zamowienie_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('zamowienie_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}